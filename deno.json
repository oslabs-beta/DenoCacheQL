{
  "compilerOptions": {
    "jsx": "react",
    "jsxFactory": "React.createElement",
    "module": "commonjs",
    "jsxFragmentFactory": "React.Fragment",
    "target": "es6",
    "lib" : 
      ["dom", 
      "es6" ,
      "dom.iterable",
      "esnext",
      "ScriptHost",
      "deno.ns"]
  },
  "include": ["dql_playground/**/*"]

  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "forceConsistentCasingInFileNames": true 
}